<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
   <!--Section: Live preview-->
  <section class="form-dark">

  <!--Form without header-->
    <div class="card card-image" >
      <!--Header-->
      <div class="text-center">
          <h3 class="white-text mb-3 mt-2 font-weight-bold"><strong>LOG</strong> <a class="green-text font-weight-bold"><strong> IN</strong></a></h3>
      </div>
      <!--Body-->
      <div class="md-form">
        <label class="text-body mb-3 mt-2 font-weight-bold" for="Form-email5">Your email</label>
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control white-text", id: "Form-email15"%>
      </div>
      <div class="md-form pb-3">
        <label class="text-body mb-3 mt-2 font-weight-bold" for="Form-pass5">Your password</label>
        <%= f.password_field :password, autocomplete: "current-password", class: "form-control white-text", id: "Form-pass5" %>
        <div class="form-check my-4">
          <input class="form-check-input" type="checkbox" value="" id="defaultCheck17">
          <label class="text-body mb-3 mt-2 font-weight-bold" for="defaultCheck17">Accept the<a href="#" class="green-text font-weight-bold"> Terms and Conditions</a></label>
        </div>
      </div>
      <!--Grid row-->
      <div class="row d-flex align-items-center mb-4">
        <!--Grid column-->
        <div class="text-center mb-3 col-md-12">
             <%= f.submit "Log in", class: "btn btn-success btn-block btn-rounded z-depth-1" %>
        </div>
        <!--Grid column-->
      </div>
      <!--Grid row-->
      <!--Grid column-->
      <div class="col-md-12">
          <%= render "devise/shared/links" %>
      </div>
      <!--Grid column-->
    </div>
  <!--/Form without header-->
  </section>
  <!--Section: Live preview-->

<% end %>
